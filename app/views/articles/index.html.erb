<div class="container">
    <%# テンプレの使い回し %>
    <%= render 'tabs' %>

    <%# このままだとテンプレがdoも引数を読み込まないので、テンプレ読み込みで値を渡してあげる %>
    <% @articles.each do |article| %>
        <%= render 'article', article: article %>
    <% end %>
    

    <%# new postへのリンク%>
    <%= link_to  new_article_path do %>
        <div class="float_btn">
            <i class="fa fa-plus"></i>
        </div>
    <% end %>

   
</div>